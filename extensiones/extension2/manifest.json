  {
    "manifest_version": 2,
    "name": "Pega datos",
    "description": "Pega datos",
    "version": "1.0",
    "icons": {
    "128": "icon128.png"
    },
    "background":{
        "scripts": ["background.js"]
    },
    "browser_action":{
        "default_title": "Traspaso Datos",
        "default_icon": "check.png"
    },
    "permissions": [
    "notifications",
    "storage",
    "tabs",
    "http://*/*", "https://*/*",
    "cookies",
    "https://www.youtube.com/*"
    ]
  }